{
  "name": "jaab.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db-migrate": "cd db && sequelize-cli db:migrate",
    "db-seed": "cd db && sequelize-cli db:seed:all",
    "db-unseed": "cd db && sequelize-cli db:seed:undo:all",
    "db-add-model": "cd db && sequelize-cli model:generate --name NewTable --attributes id:UUID",
    "db-add-seeder": "cd db && sequelize-cli seed:generate --name new-seeder",
    "db-add-migration": "cd db && sequelize-cli migration:generate --name new-migration"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "mysql2": "^3.11.0",
    "next": "14.2.5",
    "path": "^0.12.7",
    "pm2": "^5.4.2",
    "react": "^18",
    "react-dom": "^18",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "postcss": "^8",
    "sqlite3": "5.1.7",
    "tailwindcss": "^3.4.1"
  }
}